{
    "name": "connection",
    "version": "1.0.0",
    "description": "Developers Network",
    "main": "server.js",
    "scripts": {
        "build:prod": "./node_modules/.bin/webpack --mode production",
        "build:dev": "./node_modules/.bin/webpack --mode development",
        "prestart": "npm run build:prod",
        "start": "./node_modules/.bin/concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server --delay 25000ms",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "test": "mocha 'test/*.js'"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "author": "Daryl Bilderback",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "config": "^3.2.4",
        "cors": "^2.8.5",
        "eslint": "^6.6.0",
        "eslint-plugin-import": "^2.18.2",
        "express": "^4.17.1",
        "express-validator": "^6.2.0",
        "gravatar": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "mem": "^5.1.1",
        "mongoose": "^5.7.7",
        "request": "^2.88.0",
        "webpack": "4.40.2"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "babel-loader": "^8.0.6",
        "concurrently": "^4.1.2",
        "css-loader": "^3.2.0",
        "file-loader": "^4.2.0",
        "mocha": "^10.2.0",
        "nodemon": "^1.19.4",
        "supertest": "^4.0.2",
        "tape": "^4.11.0",
        "webpack-cli": "^3.3.10"
    }
}
